#!/bin/bash

initial_class=`hyprctl activewindow | sed -En 's/initialClass: (.*)/\1/p' | xargs`
case $initial_class in
    Alacritty)
        echo "Alacritty"
        ;;
    discord-screenaudio)
        echo "Discord"
        ;;
    *)
        initial_title=`hyprctl activewindow | sed -En 's/initialTitle: (.*)/\1/p' | xargs`
        echo $initial_title
esac